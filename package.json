{
  "name": "@whitetrefoil/vuex-type-helpers",
  "version": "0.1.2",
  "description": "Some tiny helpers to help vuex work with typescript",
  "keywords": [
    "vue",
    "vuex",
    "ts",
    "typescript"
  ],
  "main": "lib/main",
  "module": "lib/main.mjs",
  "typings": "lib/main.d.ts",
  "scripts": {
    "prepublishOnly": "node scripts/prepublish-checklist.js && npm run build",
    "build": "rimraf lib && tsc && renamer --find js --replace mjs \"lib/**\" && tsc -p tsconfig.common.json",
    "publish": "echo \"Done publishing, don't forget to:\n  1. **dist-tag** your channel in npm.\n  2. bump version in package.json\nGood luck with the publish ;)\"",
    "test": "tslint . && jest --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whitetrefoil/vuex-type-helpers.git"
  },
  "author": "WhiteTrefoil <whitetrefoil@gmail.com>",
  "license": "Unlicense",
  "files": [
    "lib/**/*.js",
    "lib/**/*.mjs",
    "lib/**/*.d.ts",
    "src",
    "README.md"
  ],
  "engines": {
    "node": ">=10.15.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.24",
    "jest": "^24.1.0",
    "renamer": "^1.1.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/~.*"
    ],
    "coverageDirectory": "test_results",
    "coverageReporters": [
      "clover",
      "lcov",
      "text-summary"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "tsConfig": "tests/tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleNameMapper": {},
    "setupFiles": [],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "preset": "ts-jest",
    "testMatch": null
  }
}
